 notifsData: data.dataNotifications,\r\n          projectsData: data.dataProjects,\r\n          tasksData: data.dataTaches,\r\n        });\r\n      })\r\n      .catch((error) => console.log(\"error\", error));\r\n  }, [setAppState]);\r\n  return (\r\n    <React.Fragment>\r\n      <main className=\"App\">\r\n        <Router>\r\n          <Sidebar />\r\n\r\n          <Switch>\r\n            <Route\r\n              exact\r\n              path=\"/\"\r\n              render={() => (\r\n                <HomeLoading\r\n                  isLoading={appState.loading}\r\n                  Userdata={appState.userData}\r\n                  tasks={appState.tasksData}\r\n                />\r\n              )}\r\n            />\r\n            <Route\r\n              path=\"/projects\"\r\n              render={() => (\r\n                <Projects\r\n                  Userdata={appState.userData}\r\n                  projects={appState.projectsData}\r\n                />\r\n              )}\r\n            />\r\n            <Route\r\n              path=\"/notifications\"\r\n              render={() => <Notifs notifs={appState.notifsData} />}\r\n            />\r\n            <Route path=\"/explore\" component={Explore} />\r\n            <Route path=\"/settings\" component={ProjectForm} />\r\n            <Route exact path=\"/login\" component={LoginForm} />\r\n            <Route exact path=\"/signup\" component={SignUp} />\r\n          </Switch>\r\n        </Router>\r\n      </main>\r\n    </React.Fragment>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\HP\\OneDrive\\Desktop\\GenieLogiciel\\frontend\\src\\components\\TasksData.js",[],"C:\\Users\\HP\\OneDrive\\Desktop\\GenieLogiciel\\frontend\\src\\components\\loginForm.js",[],"C:\\Users\\HP\\OneDrive\\Desktop\\GenieLogiciel\\frontend\\src\\components\\Sidebar.js",["92"],"import React, { useState, useCallback, useEffect } from \"react\";\r\nimport * as FAIcons from \"react-icons/fa\";\r\nimport * as AIIcons from \"react-icons/